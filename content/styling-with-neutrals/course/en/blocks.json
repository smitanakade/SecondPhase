  {
    "_id": "59152278ad04f4d3201a7b6e",
    "_courseId": "59151f74ad04f4d3201a7b02",
    "_parentId": "59151f74ad04f4d3201a7b0b",
    "_sortOrder": 0,
    "__v": 0,
    "displayTitle": "",
    "body": "",
    "_refId": "59152278ad04f4d3201a7b6e",
    "_courseAttainment": {
