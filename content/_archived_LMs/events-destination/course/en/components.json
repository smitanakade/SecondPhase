    "_refId": "5a4b1055ec1ad576d99ed76a",
    "_numberOfColumns": 2,
    "body": "<p style=\"text-align: center;\">Flip the cards to discover the benefits of the Events Hub to the customer and to you and Myer.</p>",
    "_items": [
      {
        "frontTextStyles": {
          "_fontColor": "@swatch-24",
          "_fontSize": "36",
          "_textAlign": "center"
        },
        "_frontContentGraphic": {
          "_isBottomPosition": false
