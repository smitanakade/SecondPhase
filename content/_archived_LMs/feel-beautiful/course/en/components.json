  {
    "_id": "5a78c7cc3865607d6dca0ffc",
    "_component": "accordion",
    "_layout": "full",
    "_courseId": "5a78c7cc3865607d6dca0fed",
    "_parentId": "5a78c7cc3865607d6dca0ffb",
    "__v": 0,
