        "_backContentGraphic": {
          "_isBottomPosition": false
        },
        "frontText": "EMBRACE",
        "_id": "598bdb39ebca4a4008794ee1",
        "itemIndex": 1,
        "backText": "Embrace what makes YOU feel beautiful and encourage customers to embrace what makes them feel beautiful too!<div><br><i><br></i><i>\"Our eye colour is almost identical! &nbsp;I use this shade all the time - it would really bring out your eyes!\"</i></div>"
      },
      {
        "frontTextStyles": {
          "_fontColor": "@swatch-17",
          "_fontSize": "40"
        },
        "_frontGraphic": {
          "_id": "599ceabeebca4a4008795089",
