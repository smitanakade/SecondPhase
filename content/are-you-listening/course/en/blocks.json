    "_parentId": "58b64322ed2155973abb7e05",
    "_sortOrder": 1,
    "__v": 0,
    "displayTitle": "",
    "body": "<p style=\"text-align: center;\">The final step is to summarise what you’ve heard.<br></p>",
    "_refId": "58b64322ed2155973abb7e0a",
    "_courseAttainment": {
      "_isEnabled": false
    },
    "_componentsVerticalAlignment": "top",
    "_singleWidthIsCentered": true,
    "_singleWidthComponentLayout": "default",
    "_classes": "",
    "_displayTitleSettings": {
      "_textAlign": "center"
    },
    "title": "New Block Title",
