    "_singleWidthIsCentered": true,
    "_singleWidthComponentLayout": "default",
    "_classes": "",
    "title": "Blazers, Shirts, Chinos",
    "_type": "block",
    "_courseOrder": "0-3-1"
  },
  {
    "_id": "5a4ad403ec1ad576d99ed163",
    "_sortOrder": 2,
    "_parentId": "5a4ad403ec1ad576d99ed15e",
    "_courseId": "5a4ad403ec1ad576d99ed13c",
    "__v": 0,
    "_refId": "59b8942e5dfae0c5453cd4c5",
    "displayTitle": "Colours",
    "body": "<div style=\"text-align: center;\">This season’s colours are blue and grey with pops of pink and lilac - these colours flatter any skin tone and liven up a suit.<br></div>",
    "_courseAttainment": {
      "_isEnabled": false
    },
    "_componentsVerticalAlignment": "top",
