  {
    "_id": "5a53e76f3865607d6dc9f2de",
    "_component": "textReveal",
    "_layout": "full",
    "_courseId": "5a4aca25ec1ad576d99ed05a",
    "_parentId": "5a53e7643865607d6dc9f2dc",
