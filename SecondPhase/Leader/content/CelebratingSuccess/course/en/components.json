ouldEnableGraphicText":false,"_interactionLabel":{"_location":"before widget"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_reflection":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_hint":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_courseAssessment":{"_isEnabled":false},"_assignment":{"feedback":"","_isCorrect":false,"_isMarked":false,"_isEnabled":false},"_badges":{"_badgeIds":[]},"_points":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_links":{"_isEnabled":false},"_classes":"","title":"Graphic Title","_type":"component","_courseOrder":"0-9-0-0"},{"_id":"5b757e5bd4b34e50d75fedd8","_component":"text","_layout":"right","_courseId":"5b757e5ad4b34e50d75fedac","_parentId":"5b757e5bd4b34e50d75fedd6","__v":0,"_refId":"5b237e5c20df6d53937e05ea","body":"<p style=\"text-align: center;\">Celebrating success encourages an environment where people feel like they belong.</p><p style=\"text-align: center;\"><br></p><p style=\"text-align: center;\">We know that when people feel like they belong, they tend to enjoy what they do more, achieve better results, and feel more satisfied at work.</p><p style=\"text-align: center;\"><br></p><p style=\"text-align: center;\">A great example is Airbnb, who celebrate success by hosting pop-up birthday parties, baby showers, and award ceremonies.</p><p style=\"text-align: center;\"><br></p><p style=\"text-align: center;\">Airbnb&rsquo;s Global Head of Employee Experience, Mark Levy, describes their way of celebrating as &rdquo;simply, thoughtful and caring people paying attention - we remember that we need to have a full life.&rdquo;</p><p style=\"text-align: center;\"><br></p><p style=\"text-align: center;\">It&rsquo;s no surprise that Airbnb was rated the best place to work in 2016 by Glassdoor.</p>","_interactionLabel":{"_location":"before body"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_reflection":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_hint":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_courseAssessment":{"_isEnabled":false},"_assignment":{"feedback":"","_isCorrect":false,"_isMarked":false,"_isEnabled":false},"_badges":{"_badgeIds":[]},"_points":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_links":{"_isEnabled":false},"_classes":"","title":"Text Title","_type":"component","_courseOrder":"0-9-0-1"},{"_id":"5b7587f3d4b34e50d75fefad","_refId":"5b7587f3d4b34e50d75fefad","_courseId":"5b757e5ad4b34e50d75fedac","_parentId":"5b7587f3d4b34e50d75fefa1","_layout":"full","_component":"text","_interactionLabel":{"_location":"before body"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"__v":0,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_reflection":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_hint":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_courseAssessment":{"_isEnabled":false},"_assignment":{"feedback":"","_isCorrect":false,"_isMarked":false,"_isEnabled":false},"_badges":{"_badgeIds":[]},"_points":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_links":{"_isEnabled":false},"_classes":"","title":"Text Title","_type":"component","_courseOrder":"0-10-0-0"},{"_id":"5b757e5bd4b34e50d75feddc","_component":"text","_layout":"full","_courseId":"5b757e5ad4b34e50d75fedac","_parentId":"5b757e5bd4b34e50d75feddb","__v":0,"_refId":"5b45a975ee774f335893fd21","body":"<p>✔ &nbsp;Share your small wins at the end of each week at a team meeting.</p><p><br></p><p>✔&nbsp; Write thank you notes.</p><p><br></p><p>✔&nbsp; Schedule an early breakfast or lunch outing that fits in with everyone (i.e. includes people with kids or other commitments).</p><p><br></p><p>✔&nbsp; Write an email to recognise people (cc&rsquo; in senior leaders).</p><p><br></p><p>✔&nbsp; Publicly recognise a team member who role models our values or has done inspirational work.</p><p><br></p><p>✔&nbsp; Organise a morning tea to share success stories.</p><p><br></p><p>✔&nbsp; Go for a coffee to celebrate or mark the occasion.</p><p><br></p><p>✔&nbsp; Give a certificate or trophy to acknowledge contribution and great achievement.</p><p><br></p><p>✔ &nbsp;Recognise your colleague using one of Myer&rsquo;s formal recognition processes i.e. M-Metrics (MyWallet), &nbsp;Inspirational People Awards, Post on Yammer etc.</p>","_interactionLabel":{"_location":"before body"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_reflection":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_hint":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_courseAssessment":{"_isEnabled":false},"_assignment":{"feedback":"","_isCorrect":false,"_isMarked":false,"_isEnabled":false},"_badges":{"_badgeIds":[]},"_points":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_links":{"_isEnabled":false},"_classes":"","title":"Text Title","_type":"component","_courseOrder":"0-11-0-0"},{"_id":"5b757e5bd4b34e50d75fedde","_component":"links","_layout":"full","_courseId":"5b757e5ad4b34e50d75fedac","_parentId":"5b757e5bd4b34e50d75feddd","__v":0,"_refId":"5b34847012be8f32bf88a84c","instruction":"","body":"<p style=\"text-align: center;\">Download this list to use at work.</p><p style=\"text-align: center;\"><br></p>","_animations":{"_easing":"default","_scaleIn":{"_from":50,"_isEnabled":false},"_fadeIn":{"_from":0,"_isEnabled":false},"_slideInY":{"_from":50,"_isReverseDirection":false,"_isEnabled":false},"_slideInX":{"_from":50,"_isReverseDirection":false,"_isEnabled":false}},"_items":[{"_isImageLink":false,"_linkTarget":"new window","_shouldCloseWindow":false,"_shouldCompleteComponentsInPage":false,"_components"