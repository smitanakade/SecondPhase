a449587c9beb0a93","_parentId":"5b7fe252a449587c9beb0ac5","__v":0,"_refId":"5b6181f9d2fd871ce0df86f6","_showNavOnTop":true,"_animations":{"_easing":"default","_scaleIn":{"_from":50,"_isEnabled":false},"_fadeIn":{"_from":0,"_isEnabled":true},"_slideInY":{"_from":50,"_isReverseDirection":false,"_isEnabled":false},"_slideInX":{"_from":50,"_isReverseDirection":false,"_isEnabled":false}},"_itemLayout":{"small":100,"medium":50,"large":50},"_items":[{"_graphic":{"src":{"_size":"small","_type":"image","_extension":"png","_id":"5b629890c973d423591c532f"},"caption":"<span style=\"color: rgb(255, 255, 255);\">MORE LOW IMPACT DECISIONS</span>"},"_revealGraphicLayout":"top","_shouldSetCompletion":false,"text":"<p style=\"text-align: center;\">Try to automate and outsource low impact decisions where possible. For example, Barack Obama and Steve Jobs had only two to three different outfits. This meant that they didn&rsquo;t have to invest time and energy into making a decision on what to wear each day, allowing them to conserve energy to make more high impact decisions. Consider which low-impact decisions you could automate in your work and life.</p>","itemIndex":0,"_id":"5b61820ed2fd871ce0df86f9"},{"_graphic":{"src":{"_size":"small","_type":"image","_extension":"png","_id":"5b629890c973d423591c532f"},"caption":"<span style=\"color: rgb(255, 255, 255);\">MORE HIGH IMPACT DECISIONS</span>"},"_revealGraphicLayout":"top","_shouldSetCompletion":false,"text":"<p style=\"text-align: center;\">You need time to consider the high impact decisions fully. To avoid decision fatigue, check whether you can delay the decision and give yourself time to think it over in more depth. You could also create a small working group who could advise you on the best choice from their perspective, when required to make a high impact decision at short notice.</p>","itemIndex":1,"_id":"5b618247d2fd871ce0df86fc"}],"_shouldDisableItemTitles":false,"_shouldCenterCaption":true,"_shouldDisplayCaptionOverGraphic":true,"_shouldShowMarking":false,"_isExploratoryQuestion":false,"_interactionLabel":{"_location":"before widget"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_reflection":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_hint":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_courseAssessment":{"_isEnabled":false},"_assignment":{"feedback":"","_isCorrect":false,"_isMarked":false,"_isEnabled":false},"_badges":{"_badgeIds":[]},"_points":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_links":{"_isEnabled":false},"_classes":"","title":"Text Reveal Title","_type":"component","_courseOrder":"0-10-0-0"},{"_id":"5b80e1c350822b17e1e5db60","_refId":"5b80e1c350822b17e1e5db60","_courseId":"5b7fe251a449587c9beb0a93","_parentId":"5b80e1c350822b17e1e5db54","_layout":"full","_component":"text","_interactionLabel":{"_location":"before body"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"__v":0,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_reflection":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_hint":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_courseAssessment":{"_isEnabled":false},"_assignment":{"feedback":"","_isCorrect":false,"_isMarked":false,"_isEnabled":false},"_badges":{"_badgeIds":[]},"_points":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_links":{"_isEnabled":false},"_classes":"","title":"Text Title","_type":"component","_courseOrder":"0-11-0-0"},{"_id":"5b7fe252a449587c9beb0ac9","_component":"textNarrative","_layout":"full","_courseId":"5b7fe251a449587c9beb0a93","_parentId":"5b7fe252a449587c9beb0ac8","__v":0,"_refId":"5b618526d2fd871ce0df871d","_animations":{"_easing":"default","_scaleIn":{"_from":50,"_isEnabled":false},"_fadeIn":{"_from":10,"_isEnabled":true},"_slideIn":true},"_canPulsate":true,"_items":[{"_narrativeTextLayout":"center","body":"<p><span style=\"color: rgb(255, 255, 255);\">Create the right foundations before delegating to others in your team. This means having the right people, being clear on their role and how you want things done as well as developing strong metrics to keep track of progress. Make sure the team puts the customer first and taps into our organisational values when making decisions.</span></p><p><span style=\"color: rgb(255, 255, 255);\"><br></span></p><p><span style=\"color: rgb(255, 255, 255);\">Remember as a minimum to include metrics that reflect generation of revenue, the saving of costs and customer satisfaction.</span></p>","itemIndex":0,"_id":"5b6185cad2fd871ce0df8721","title":"<strong>PHASE 1: SET THE FOUNDATIONS</strong>"},{"body":"<p><span style=\"color: rgb(255, 255, 255);\">Do you find team members often bring you problems to solve?</span></p><p><span style=\"color: rgb(255, 255, 255);\"><br></span></p><p><span style=\"color: rgb(255, 255, 255);\">Phase 2 of delegating decision making is to ask team members to bring you a short list of options to help solve the problem. Encourage them to define if the decision is low or high impact. You make the ultimate decision, however this gives you the opportunity to coach others around their decision making, as well as assess their ideas and decision making styles. Both of which will prepare you for Phase 3.</span></p>","itemIndex":1,"_id":"5b6185f1d2fd871ce0df8727","title":"<strong>PHASE 2: SUPPORT TEAM MEMBERS TO FIND SOLUTIONS</strong>","_narrativeTextLayout":"center"},{"_narrativeTextLayout":"center","title":"<strong>PHASE 3: TEAM MEMBERS DECIDE, AND INFORM YOU BEFORE ACTING</strong>","_id":"5b61862fd2fd871ce0df872f","itemIndex":2,"body":"<p><span style=\"color: rgb(255, 255, 255);\">Your team are starting to make the decisions. For high impact decisions, encourage them to bring the decisions to you first so you can discuss the consequences with them.</span></p><p><span style=\"color: rgb(255, 255, 255);\"><br></span></p><p><span style=\"color: rgb(255, 255, 255);\">For low impact decisions, try to refrain from intervening unless the consequences of the decision might escalate. If you find yourself frequently needing to intervene, you probably need to go back to Phase 2.</span></p>"},{"_narrativeTextLayout":"center","title":"<strong>PHASE 4: TEAM MEMBERS DECIDE AND ACT, YOU ARE KEPT UP-TO-DATE</strong>","_id":"5b61865fd2fd871ce0df8735","itemIndex":3,"body":"<p><span style=\"color: rgb(255, 255, 255);\">Your team are now making high and low impact decisions and are checking-in with you regularly. It&rsquo;s best to move to this stage when you feel comfortable team members are dealing with high impact decisions well. Continue to help team members grow their decision making styles and approach.</span></p>"}],"prevButtonAriaLabel":"previous slide","nextButtonAriaLabel":"next slide","_interactionLabel":{"_location":"before widget"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_reflection":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_hint":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_courseAssessment":{"_isEnabled":false},"_assignment":{"feedback":"","_isCorrect":false,"_isMarked":false,"_isEnabled":false},"_badges":{"_badgeIds":[]},"_points":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_links":{"_isEnabled":false},"_classes":"","title":"Narrative Title","_type":"component","_courseOrder":"0-12-0-0"},{"_id":"5b80e1c350822b17e1e5db61","_refId":"5b80e1c350822b17e1e5db61","_courseId":"5b7fe251a449587c9beb0a93","_parentId":"5b80e1c350822b17e1e5db55","_layout":"full","_component":"text","_interactionLabel":{"_location":"before body"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"__v":0,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_refle