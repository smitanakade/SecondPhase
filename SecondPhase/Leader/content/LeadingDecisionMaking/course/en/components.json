itle":"","body":"","_animations":{"_easing":"default","_scaleIn":{"_from":50,"_isEnabled":false},"_fadeIn":{"_from":0,"_isEnabled":false},"_slideInY":{"_from":50,"_isReverseDirection":false,"_isEnabled":false},"_slideInX":{"_from":50,"_isReverseDirection":false,"_isEnabled":false}},"_items":[{"frontTextStyles":{"_fontColor":"transparent","_fontSize":"25"},"_frontContentGraphic":{"src":{"_type":"image","_extension":"png","_id":"5b616915d2fd871ce0df85fd"},"_isBottomPosition":false},"backTextStyles":{"_fontColor":"transparent"},"_backContentGraphic":{"_isBottomPosition":false},"backText":"<p style=\"text-align: center;\"><span style=\"color: rgb(255, 255, 255); font-size: 14px;\">Decision making with your head involves considering the big picture and strategic priorities for you and your team. This, in combination with analysing data and facts, will help you to use logic and &lsquo;smarts&rsquo; to inform your choice. </span></p>","itemIndex":0,"_id":"5b0e87521faab831a06f9f2f","frontText":"<p style=\"text-align: center;\"><span style=\"color: rgb(255, 255, 255);\">HEAD</span></p>"},{"frontTextStyles":{"_fontColor":"transparent","_fontSize":"25"},"_frontContentGraphic":{"src":{"_type":"image","_extension":"png","_id":"5b616929d2fd871ce0df8600"},"_isBottomPosition":false},"backTextStyles":{"_fontColor":"transparent"},"_backContentGraphic":{"_isBottomPosition":false},"_id":"5b0e87541faab831a06f9f30","itemIndex":1,"frontText":"<p style=\"text-align: center;\"><span style=\"color: rgb(255, 255, 255);\">HEART</span></p>","backText":"<p style=\"text-align: center;\"><span style=\"font-size: 14px;\">Decision making using your heart is all about considering the impact of your decisions on those around you. This means your team, your people leader, and your customers. The best way to do this is to put yourself in their shoes, considering how they would feel and react to your decision. Decision making with your heart also involves building trusted relationships with others, and collaborating with them to seek their advice. </span></p>"},{"frontTextStyles":{"_fontColor":"transparent","_fontSize":"25"},"_frontContentGraphic":{"src":{"_type":"image","_extension":"png","_id":"5b61693bd2fd871ce0df8604"},"_isBottomPosition":false},"backTextStyles":{"_fontColor":"transparent"},"_backContentGraphic":{"_isBottomPosition":false},"itemIndex":2,"_id":"5b0e87541faab831a06f9f31","frontText":"<p style=\"text-align: center;\"><span style=\"color: rgb(255, 255, 255);\">GUTS</span></p>","backText":"<p style=\"text-align: center;\"><span style=\"color: rgb(255, 255, 255); font-size: 14px;\">Gut based decision making is about using your instinct combined with calculated risk to make courageous choices. This might involve challenging the status quo or taking decisive action where speed is of the essence. It also means reflecting on our organisational values to help make the right decision in tough or ambigous situations.</span></p>"}],"_flipCardItemContentAlignment":"center","_flipCardsContainerAlignment":"left","_flipCardsContainerMaxWidth":"1024px","_canSetContainerMaxWidth":true,"_backTextStyles":{"_fontColor":"transparent"},"_frontTextStyles":{"_fontColor":"transparent"},"_backGraphic":{"_type":"image","_extension":"png","_id":"5b629918c973d423591c5341"},"_frontGraphic":{"_type":"image","_extension":"png","_id":"5b7a58d17eff8b5899e26fb9"},"_shouldFlipBackNonSelectedCards":true,"_shouldUseIndividualItemImages":false,"_shouldUseFrontImageAsBackImage":false,"_interactionLabel":{"_location":"before widget"},"_isInteractionLabelEnabled":false,"_isQuestionType":false,"_tags":{"_tagsList":[],"_isEnabled":false},"_stamps":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_reflection":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_hint":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_courseAssessment":{"_isEnabled":false},"_assignment":{"feedback":"","_isCorrect":false,"_isMarked":false,"_isEnabled":false},"_badges":{"_badgeIds":[]},"_poin